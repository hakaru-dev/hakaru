def exponential(alpha prob):
	X <~ uniform(0,1)
	return -1 * alpha * log(real2prob(X))

def weibull(alpha prob, beta prob):
	X <~ exponential(alpha)
	return real2prob(X) ** (1/beta)

def Rayleigh(alpha prob):
	X <~ weibull(alpha,2)
	return X


def Rayleigh_to_ChiSq():
	x <~ Rayleigh(1)
	return prob2real(x ** 2)

Rayleigh_to_ChiSq()